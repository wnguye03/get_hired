{
    "name": "Get Hired",
    "version": "0.1.0",
    "description": "Helping you keep track of all your job applications",
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
    ],
    "action": {
      "default_icon": {
        "16": "assets/ext-icon.png",
        "24": "assets/ext-icon.png",
        "32": "assets/ext-icon.png"
      },
      "default_title": "Get Hired",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}